modules = ["python-3.11", "web", "bash"]
[agent]
expertMode = true
integrations = ["google-sheet:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["chromedriver", "chromium", "geckodriver", "glibcLocales"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Dashboard"

[[workflows.workflow]]
name = "Dashboard"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python dashboard.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 36913
externalPort = 3000

[[ports]]
localPort = 38811
externalPort = 3001

[deployment]
deploymentTarget = "vm"
run = ["python", "dashboard.py"]
